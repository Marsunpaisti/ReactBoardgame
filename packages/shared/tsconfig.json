{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "outDir": "./build"
  },
  "include": ["src"]
}
